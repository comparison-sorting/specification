{
  "name": "@aureooms/js-in-situ-sort-spec",
  "description": "in place sorting code bricks for JavaScript",
  "version": "7.0.2",
  "author": "Aur√©lien Ooms <aurelien.ooms@gmail.com>",
  "ava": {
    "require": [
      "regenerator-runtime/runtime",
      "@babel/register"
    ],
    "files": [
      "test/src/**/*"
    ],
    "timeout": "1m"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-in-situ-sort-spec/issues"
  },
  "dependencies": {
    "@aureooms/js-array": "^4.0.0",
    "@aureooms/js-compare": "^1.4.5",
    "@aureooms/js-functools": "^2.0.3",
    "@aureooms/js-itertools": "^3.1.1",
    "@aureooms/js-random": "^2.0.0",
    "@aureooms/js-sort": "^7.0.0"
  },
  "devDependencies": {
    "ava": "^3.11.1",
    "babel-cli": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-latest": "^6.22.0",
    "codeclimate-test-reporter": "^0.4.1",
    "coveralls": "^2.11.16",
    "esdoc": "^1.1.0",
    "nyc": "^10.1.2",
    "@babel/cli": "7.10.5",
    "@babel/core": "7.11.1",
    "@babel/preset-env": "7.11.0",
    "@babel/register": "7.10.5",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-plugin-unassert": "3.0.1",
    "babel-preset-power-assert": "3.0.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "np": "6.3.2",
    "regenerator-runtime": "0.13.5"
  },
  "homepage": "http://aureooms.github.io/js-in-situ-sort-spec",
  "keywords": [
    "in situ",
    "javascript",
    "js",
    "sorting",
    "specification"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "url": "https://github.com/aureooms/js-in-situ-sort-spec.git",
    "type": "git"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "test": "ava",
    "esdoc": "esdoc",
    "prepare": "npm run build"
  }
}
